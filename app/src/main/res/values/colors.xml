<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- НАБОР ЦВЕТОВ ИЗ figma -->
    <color name="yp_blue_light">#9FBBF3</color>              <!--  ███   ███  █  █  █    -->
    <color name="yp_blue">#3772E7</color>                    <!--  █ █   █    █  █  █    -->
    <color name="yp_white">#FFFFFF</color>                   <!--  █ █   ███  ████  ████ -->
    <color name="yp_text_gray">#AEAFB4</color>               <!-- █████  █    █  █  █  █ -->
    <color name="yp_light_gray">#E6E8EB</color>              <!-- █   █  ███  █  █  ████ -->
    <color name="alex_gray">#373737</color>
    <color name="yp_black">#1A1B22</color>
    <color name="black">#FF000000</color>
    <color name="mount_thumb_color">#3F51B5</color> <!-- Розовый с альфой 0.8 -->
    <color name="kototeka_thumb_color">#C94E3A</color> <!-- Оранжевый с альфой 0.8 -->

    <!-- ЦВЕТОВЫЕ ПАРЫ ДЛЯ ТЕМ И СТИЛЕЙ (день)-->
    <!-- цвет фона главного меню -->
    <color name="yp_blue_light__yp_black">@color/yp_blue_light</color>
    <!-- цвет кнопок главного меню -->
    <color name="yp_white__yp_white">@color/yp_white</color>
    <!-- цвет шрифта главного меню, иконок главного меню -->
    <color name="yp_black__yp_black">@color/yp_black</color>
    <!-- цвет иконок с ошибкой, цвет шрифта вторичного меню, название песен-->
    <color name="yp_black__yp_white">@color/yp_black</color>
    <!-- цвет кнопок вторичного меню, фон вторичного меню -->
    <color name="yp_white__yp_black">@color/yp_white</color>
    <!-- цвет иконок вторичного меню, имени автора и продолжительности трека -->
    <color name="yp_text_gray__yp_white">@color/yp_text_gray</color>
    <!-- цвет тумблера свитч, цвет поисковой строки и текста в ней -->
    <color name="yp_text_gray__yp_blue">@color/yp_text_gray</color>
    <color name="yp_light_gray__yp_blue_light">@color/yp_light_gray</color>
    <!-- цвет поисковой строки с текстом и иконками -->
    <color name="yp_text_gray__yp_black">@color/yp_text_gray</color>
    <!-- цвет подложки для закругления углов поисковой строки -->
    <color name="yp_light_gray__yp_white">@color/yp_light_gray</color>
    <!-- новый цвет для иконок меню настроек -->
    <color name="alex_gray__yp_white">@color/alex_gray</color>

</resources>


